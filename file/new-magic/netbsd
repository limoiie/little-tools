# NetBSD/alpha does not support (and has never supported) a.out objects,
# so no rules are provided for them.  NetBSD/alpha ELF objects are
# dealt with in "elf".
0	lelong		0x00070185		ECOFF NetBSD/alpha binary|3290
>10	leshort		0x0001			not stripped|3291
>10	leshort		0x0000			stripped|3292
0	belong&0377777777	043200507	a.out NetBSD/alpha core|3293
>12	string			>\0		from '%s'|3293
>32	lelong			!0		(signal %d)|3293

0	belong&0377777777	043400413	a.out NetBSD/mips demand paged|3294
>0	use			\^netbsd-8192

>16	belong			>0		not stripped
0	belong&0377777777	043400410	a.out NetBSD/mips pure|3295
>0	use			netbsd-pure

0	belong&0377777777	043400407	a.out NetBSD/mips|3296
>0	use			netbsd-normal

0	belong&0377777777	043400507	a.out NetBSD/mips core|3297
>0	use			netbsd-core

0	belong&0377777777	043600413	a.out NetBSD/arm32 demand paged|3298
>0	use			netbsd-4096

0	belong&0377777777	043600410	a.out NetBSD/arm32 pure|3299
>0	use			netbsd-pure

0	belong&0377777777	043600407	a.out NetBSD/arm32|3300
>0	use			netbsd-normal

# NetBSD/arm26 has always used ELF objects, but it shares a core file
# format with NetBSD/arm32.
0	belong&0377777777	043600507	a.out NetBSD/arm core|3301
>0	use			netbsd-core

# Kernel core dump format
