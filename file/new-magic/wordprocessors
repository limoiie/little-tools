>>>9	byte	x	Corel LetterPerfect: Unknown filetype %d|4577
# Corel Terminal
>8	byte	17
>>9	byte	10	Corel Terminal resource data|4578
>>9	byte	11	Corel Terminal resource data|4578
>>9	byte	43	Corel Terminal resource data|4578
>>9	default	x
>>>9	byte	x	Corel Terminal: Unknown filetype %d|4579
# Corel loadable file
>8	byte	18
>>9	byte	10	Corel loadable file|4580
>>9	byte	11	Corel GUI loadable text|4581
>>9	byte	12	Corel graphics resource data|4582
>>9	byte	13	Corel printer settings file|4583
>>9	byte	14	Corel port definition file|4584
>>9	byte	15	Corel print queue parameters|4585
>>9	byte	16	Corel compressed file|4586
>>9	default	x
>>>9	byte	x	Corel loadable file: Unknown filetype %d|4587
>>15	byte	0	\b, optimized for Intel
>>15	byte	1	\b, optimized for Non-Intel
# Network service
>8	byte	20
>>9	byte	10	Corel Network service msg file|4588
>>9	byte	11	Corel Network service msg file|4588
>>9	byte	12	Corel Async gateway login msg|4589
>>9	byte	14	Corel GroupWise message file|4590
>>9	default	x
>>>9	byte	x	Corel Network service: Unknown filetype %d|4591
# GroupWise
>8	byte	31
>>9	byte	20	GroupWise admin domain database|4592
>>9	byte	21	GroupWise admin host database|4593
>>9	byte	23	GroupWise admin remote host database|4594
>>9	byte	24	GroupWise admin ADS deferment data file|4595
>>9	default	x
>>>9	byte	x	GroupWise: Unknown filetype %d|4596
# IntelliTAG
>8	byte	33
>>9	byte	10	IntelliTAG (SGML) compiled DTD|4597
>>9	default	x
>>>9	byte	x	IntelliTAG: Unknown filetype %d|4598
# everything else
>8	default x
>>8	byte	x	Unknown Corel/Wordperfect product %d,|4599
>>>9	byte	x	file type %d|4599
>10	byte	0	\b, v5.
>10	byte	!0	\b, v%d.
>11	byte	x	\b%d

# Hangul (Korean) Word Processor File
0	string	HWP\ Document\ File	Hangul (Korean) Word Processor File 3.0|4600
# From: Won-Kyu Park <wkpark@kldp.org>
512	string		R\0o\0o\0t\0	Hangul (Korean) Word Processor File 2000|4601
!:mime	application/x-hwp

# CosmicBook, from Benoit Rouits
0       string  CSBK    Ted Neslson's CosmicBook hypertext file|4602

2       string  EYWR    AmigaWriter file|4603

# chi:  file(1) magic for ChiWriter files
0       string          \\1cw\          ChiWriter file|4604
>5      string          >\0             version %s|4604
0       string          \\1cw           ChiWriter file|4604

# Quark Express from http://www.garykessler.net/library/file_sigs.html
2	string	IIXPR3			Intel Quark Express Document (English)|4605
2	string	IIXPRa			Intel Quark Express Document (Korean)|4606
2	string	MMXPR3			Motorola Quark Express Document (English)|4607
!:mime	application/x-quark-xpress-3
2	string	MMXPRa			Motorola Quark Express Document (Korean)|4608

# adobe indesign (document, whatever...) from querkan
0	belong	0x0606edf5		Adobe InDesign|4609
>16	string	DOCUMENT		Document|4610

#------------------------------------------------------------------------------
# ichitaro456: file(1) magic for Just System Word Processor Ichitaro
#
# Contributor kenzo-:
# Reversed-engineered JS Ichitaro magic numbers
#

0	string		DOC
>43	byte		0x14	Just System Word Processor Ichitaro v4|4611
!:mime	application/x-ichitaro4
>144	string	JDASH		application/x-ichitaro4|4611

0	string		DOC
>43	byte		0x15	Just System Word Processor Ichitaro v5|4612
!:mime	application/x-ichitaro5

0	string		DOC
>43	byte		0x16	Just System Word Processor Ichitaro v6|4613
!:mime	application/x-ichitaro6

# Type: Freemind mindmap documents
# From: Jamie Thompson <debian-bugs@jamie-thompson.co.uk>
0	string/w	\<map\ version	Freemind document|4614
!:mime	application/x-freemind

# Type: Freeplane mindmap documents
# From: Felix Natter <fnatter@gmx.net>
0       string/w        \<map\ version="freeplane  Freeplane document|4615
!:mime  application/x-freeplane

# Type:        Scribus
# From:        Werner Fink <werner@suse.de>
0	string	\<SCRIBUSUTF8\ Version		Scribus Document|4616
0	string	\<SCRIBUSUTF8NEW\ Version	Scribus Document|4616
!:mime	application/x-scribus

# help files .hlp compiled from html and used by gfxboot added by Joerg Jenderek
# markups page=0x04,label=0x12, followed by strings like "opt" or "main" and title=0x14
0	ulelong&0x8080FFFF	0x00001204	gfxboot compiled html help file|4617
