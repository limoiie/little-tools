>0x00003B	byte		0x02		(real vector)|4194
>0x00003B	byte		0x03		(complex vector)|4195
>0x00003B	byte		0x04		(real list)|4196
>0x00003B	byte		0x05		(complex list)|4197
>0x00003B	byte		0x06		(real matrix)|4198
>0x00003B	byte		0x07		(complex matrix)|4199
>0x00003B	byte		0x08		(real constant)|4200
>0x00003B	byte		0x09		(complex constant)|4201
>0x00003B	byte		0x0A		(equation)|4202
>0x00003B	byte		0x0C		(string)|4203
>0x00003B	byte		0x0D		(function GDB)|4204
>0x00003B	byte		0x0E		(polar GDB)|4205
>0x00003B	byte		0x0F		(parametric GDB)|4206
>0x00003B	byte		0x10		(diffeq GDB)|4207
>0x00003B	byte		0x11		(picture)|4208
>0x00003B	byte		0x12		(program)|4209
>0x00003B	byte		0x13		(range)|4210
>0x00003B	byte		0x17		(window settings)|4211
>0x00003B	byte		0x18		(window settings)|4211
>0x00003B	byte		0x19		(window settings)|4211
>0x00003B	byte		0x1A		(window settings)|4211
>0x00003B	byte		0x1B		(zoom)|4212
>0x00003B	byte		0x1D		(backup)|4213
>0x00003B	byte		0x1E		(unknown)|4214
>0x00003B	byte		0x2A		(equation)|4202
>0x000032	string		ZS4		- ZShell Version 4 File.
>0x000032	string		ZS3		- ZShell Version 3 File.
