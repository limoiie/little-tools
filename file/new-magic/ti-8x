>0x00003B	byte		0x00		(real number)|4182
>0x00003B	byte		0x01		(complex number)|4183
>0x00003B	byte		0x02		(real vector)|4184
>0x00003B	byte		0x03		(complex vector)|4185
>0x00003B	byte		0x04		(real list)|4186
>0x00003B	byte		0x05		(complex list)|4187
>0x00003B	byte		0x06		(real matrix)|4188
>0x00003B	byte		0x07		(complex matrix)|4189
>0x00003B	byte		0x08		(real constant)|4190
>0x00003B	byte		0x09		(complex constant)|4191
>0x00003B	byte		0x0A		(equation)|4192
>0x00003B	byte		0x0C		(string)|4193
>0x00003B	byte		0x0D		(function GDB)|4194
>0x00003B	byte		0x0E		(polar GDB)|4195
>0x00003B	byte		0x0F		(parametric GDB)|4196
>0x00003B	byte		0x10		(diffeq GDB)|4197
>0x00003B	byte		0x11		(picture)|4198
>0x00003B	byte		0x12		(program)|4199
>0x00003B	byte		0x13		(range)|4200
>0x00003B	byte		0x17		(window settings)|4201
>0x00003B	byte		0x18		(window settings)|4201
>0x00003B	byte		0x19		(window settings)|4201
>0x00003B	byte		0x1A		(window settings)|4201
>0x00003B	byte		0x1B		(zoom)|4202
>0x00003B	byte		0x1D		(backup)|4203
>0x00003B	byte		0x1E		(unknown)|4204
>0x00003B	byte		0x2A		(equation)|4192
>0x000032	string		ZS4		- ZShell Version 4 File.
>0x000032	string		ZS3		- ZShell Version 3 File.
